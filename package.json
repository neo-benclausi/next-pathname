      "import": "./dist/next15.js"
    }
  },
  "scripts": {
    "build": "tsup src/next14.ts src/next15.ts --format esm --dts"
  },
  "keywords": [],
  "author": "neo-benclausi",
  "license": "MIT",
  "devDependencies": {
    "next": "14",
    "tsup": "^8.3.5",
