{
  "name": "next-pathname",
  "version": "1.0.1",
  "description": "Access request pathname in Next.js RSCs",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/neo-benclausi/next-pathname"
  },
  "exports": {
